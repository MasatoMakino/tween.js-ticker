{
  "name": "@masatomakino/tween.js-ticker",
  "version": "0.2.0",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "esm"
  ],
  "description": "Simple update loop module for tweenjs/tween.js.",
  "repository": {
    "type": "git",
    "url": "git+https://MasatoMakino@github.com/MasatoMakino/tween.js-ticker.git"
  },
  "author": "MasatoMakino <unagiinu.g@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@masatomakino/raf-ticker": "^0.2.9"
  },
  "peerDependencies": {
    "@tweenjs/tween.js": ">=18.6.4 <19.0.0"
  },
  "devDependencies": {
    "@masatomakino/gulptask-demo-page": "^0.4.4",
    "@masatomakino/gulptask-dev-server": "^0.4.1",
    "@tweenjs/tween.js": "^18.6.4",
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.8.1",
    "typedoc": "^0.22.17",
    "typescript": "^4.7.3"
  },
  "scripts": {
    "prepare": "husky install",
    "start:dev": "npm run server & npm run watch:tsc & npm run watch:demo",
    "build": "npm run demo & npm run buildTS",
    "demo": "npx @masatomakino/gulptask-demo-page",
    "buildTS": "npx tsc --project tsconfig.json & tsc --project tsconfig.esm.json",
    "typedocs": "npx typedoc --out ./docs/api src/index.ts",
    "server": "npx @masatomakino/gulptask-dev-server -b ./docs/demo",
    "watch:demo": "npm run demo -- -W",
    "watch:tsc": "tsc -W --incremental --project tsconfig.json & tsc -W --incremental --project tsconfig.esm.json"
  },
  "bugs": {
    "url": "https://github.com/MasatoMakino/tween.js-ticker/issues"
  },
  "homepage": "https://github.com/MasatoMakino/tween.js-ticker#readme",
  "directories": {
    "doc": "docs",
    "lib": "lib"
  }
}
